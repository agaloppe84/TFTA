<div class="text-center">
  <h1><%= @menu.name %></h1>
  menus <%= @foodtruck.name %>
  <p>description: <%= @menu.description %></p>
  <p>photo: <%= cl_image_tag @menu.photo.path %></p>
  <p>price_cents: <%= @menu.price_cents %></p>
  <hr>
  <p><%= link_to "Editer", edit_foodtruck_menu_path(@foodtruck, @menu), class: "btn btn-primary" %></p>
