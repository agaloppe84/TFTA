  <h5><%= link_to "menus index", foodtruck_menu_path(@foodtruck, @menu), class: "btn btn-primary" %></h5>
  <h5><%= link_to "menus edit", edit_foodtruck_menu_path(@foodtruck, @menu), class: "btn btn-primary" %></h5>
  <h5><%= link_to "Delete", menu_path(@menu),method: :delete,data: { confirm: "Are you sure?" } %>
